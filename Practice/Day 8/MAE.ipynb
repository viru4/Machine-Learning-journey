   "source": [
    "Write program to compute Mean Absolute Error:\n",
    "MAE=1/n​∑∣actual−predicted∣"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "710d334c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "MAE: 3.2\n"
     ]
    }
   ],
   "source": [
    "actual = [40,50,65,80,90]\n",
    "predicted = [42,55,60,78,88]\n",
    "\n",
    "total_error = 0\n",
    "\n",
    "for i in range(len(actual)):\n",
    "    total_error += abs(actual[i] - predicted[i])\n",
    "\n",
    "mae = total_error / len(actual)\n",
    "\n",
    "print(\"MAE:\", mae)\n"
   ]
  }
